from typing import List
class Solution:
	# @param A : tuple of integers
	# @param B : tuple of integers
	# @return an integer
	def canCompleteCircuit(self, A: List[int], B: List[int])->int:
		total_gas=sum(A)
		total_cost=sum(B)
		
		if total_gas<total_cost:
			return -1
		
		start,tank=0,0
		for i in range(len(A)):
			tank+=A[i]-B[i]
			if tank<0:
				start=i+1
				tank=0
		return start
